{
  "name": "csrs-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "configure": "ts-node setenv.ts",
    "start": "yarn run configure -- --environment=dev && ng serve",
    "start-proxy": "yarn run configure -- --environment=dev && ng serve --proxy-config proxy.conf.json",
    "build-dev": "yarn run configure -- --environment=dev && ng build --source-map=false",
    "build-prod": "yarn run configure -- --environment=prod && ng build --configuration production --source-map=false",
    "test": "ng test",
    "lint": "ng lint",
    "audit": "yarn-audit-fix --dry-run",
    "audit:fix": "yarn-audit-fix",
    "e2e": "ng e2e",
    "coverage": "ng test --browsers=ChromeHeadless --watch=false --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.3",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~13.2.3",
    "@angular/compiler": "~13.2.3",
    "@angular/core": "~13.2.3",
    "@angular/forms": "~13.2.3",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~13.2.3",
    "@angular/platform-browser-dynamic": "~13.2.3",
    "@angular/router": "~13.2.3",
    "@bcgov/bc-sans": "^1.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@snowplow/browser-tracker": "^3.3.0",
    "angular-auth-oidc-client": "^13.1.0",
    "bootstrap": "^5.1.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "jquery": "^3.6.0",
    "keycloak-js": "^15.0.2",
    "ng-busy": "^12.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-progressbar": "^6.1.1",
    "rxjs": "~7.5.4",
    "ts-generic-collections-linq": "^1.0.7",
    "tslib": "^2.3.0",
    "yargs": "^17.1.0",
    "yarn": "^1.22.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.2.4",
    "@angular/cli": "13.2.4",
    "@angular/compiler-cli": "~13.2.3",
    "@types/faker": "^5.5.7",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.4.13",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "ts-node": "~10.1.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.5",
    "yarn-audit-fix": "^9.1.2"
  }
}
